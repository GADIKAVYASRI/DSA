import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int arr[]={1,2,1,1};
        int c=0;
        for(int i=0;i<arr.length;i++){
            if(arr[i]==1){
                c++;
            }
        }
        System.out.println(c);
    }
}
